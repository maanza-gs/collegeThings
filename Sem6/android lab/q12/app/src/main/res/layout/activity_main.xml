<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-black"
        android:text="Create Request"
        android:textColor="#03A9F4"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.093" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="44dp"
        android:layout_marginTop="34dp"
        android:fontFamily="sans-serif-medium"
        android:text="DATE AND TIME"
        android:textColor="#03A9F4"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="347dp"
        android:layout_height="111dp"
        android:layout_marginStart="44dp"
        android:layout_marginTop="17dp"
        android:orientation="vertical"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="56dp"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/imageView2"
                android:layout_width="30dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                app:srcCompat="@drawable/cal" />

            <com.google.android.material.textfield.TextInputEditText
                android:layout_width="324dp"
                android:layout_height="60dp"
                android:hint="Date you need babysitter"
                android:singleLine="false"
                android:textColor="#000000" />

        </LinearLayout>

        <com.google.android.material.textfield.TextInputLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/imageView3"
                    android:layout_width="105dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    app:srcCompat="@drawable/time" />

                <com.google.android.material.textfield.TextInputEditText
                    android:layout_width="176dp"
                    android:layout_height="match_parent"
                    android:hint="From"
                    tools:ignore="DuplicateClickableBoundsCheck" />

                <com.google.android.material.textfield.TextInputEditText
                    android:layout_width="147dp"
                    android:layout_height="match_parent"
                    android:hint="To" />

            </LinearLayout>
        </com.google.android.material.textfield.TextInputLayout>

    </LinearLayout>

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="45dp"
        android:layout_marginTop="38dp"
        android:fontFamily="sans-serif-medium"
        android:text="NUMBER OF KIDS"
        android:textColor="#03A9F4"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout" />

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="347dp"
        android:layout_height="72dp"
        android:layout_marginStart="45dp"
        android:layout_marginTop="10dp"
        android:orientation="vertical"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="47dp"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/imageView4"
                android:layout_width="126dp"
                android:layout_height="48dp"
                android:layout_weight="1"
                app:srcCompat="@drawable/b1g" />

            <ImageView
                android:id="@+id/imageView5"
                android:layout_width="150dp"
                android:layout_height="48dp"
                android:layout_weight="1"
                app:srcCompat="@drawable/b2g" />

            <ImageView
                android:id="@+id/imageView6"
                android:layout_width="150dp"
                android:layout_height="48dp"
                android:layout_weight="1"
                app:srcCompat="@drawable/b3g" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="24dp"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textView4"
                android:layout_width="124dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:text="1 Kid"
                android:textAlignment="center"
                android:textAllCaps="true"
                android:textColor="#98A0A0"
                tools:layout_editor_absoluteX="-175dp"
                tools:layout_editor_absoluteY="480dp" />

            <TextView
                android:id="@+id/textView5"
                android:layout_width="125dp"
                android:layout_height="match_parent"
                android:text="2 KIDS"
                android:textAlignment="center"
                android:textColor="#98A0A0" />

            <TextView
                android:id="@+id/textView6"
                android:layout_width="150dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:text="3 KIDS"
                android:textAlignment="center"
                android:textColor="#98A0A0" />
        </LinearLayout>

    </LinearLayout>

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="45dp"
        android:layout_marginTop="33dp"
        android:fontFamily="sans-serif-medium"
        android:text="CONTACT"
        android:textColor="#03A9F4"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout2" />

    <LinearLayout
        android:id="@+id/linearLayout3"
        android:layout_width="347dp"
        android:layout_height="97dp"
        android:layout_marginStart="45dp"
        android:layout_marginTop="8dp"
        android:orientation="vertical"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView7">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="52dp"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/imageView7"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                app:srcCompat="@drawable/location" />

            <com.google.android.material.textfield.TextInputEditText
                android:layout_width="309dp"
                android:layout_height="match_parent"
                android:hint="Address" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/imageView"
                android:layout_width="226dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                app:srcCompat="@drawable/phone" />

            <com.google.android.material.textfield.TextInputEditText
                android:layout_width="309dp"
                android:layout_height="55dp"
                android:hint="Phone Number" />
        </LinearLayout>
    </LinearLayout>

    <Button
        android:id="@+id/button"
        android:layout_width="347dp"
        android:layout_height="60dp"
        android:layout_marginStart="44dp"
        android:layout_marginTop="29dp"
        android:text="SEND REQUEST"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout3" />

</androidx.constraintlayout.widget.ConstraintLayout>